<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app>

    <context-param>
        <description>Location to store uploaded file</description>
        <param-name>file-upload</param-name>
        <param-value>
            C:\Users\Owner\Downloads\apache-tomcat-9.0.0.M11\webapps\data\abc
        </param-value>
